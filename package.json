{
  "name": "chainsmith-sdk",
  "type": "module",
  "version": "1.0.2-alpha.8",
  "packageManager": "pnpm@10.5.2",
  "description": "_description_",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growly-official/chainsmith-sdk.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./rpc": {
      "types": "./dist/rpc/index.d.mts",
      "default": "./dist/rpc/index.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.mts",
      "default": "./dist/types/index.mjs"
    },
    "./data": {
      "types": "./dist/data/index.d.mts",
      "default": "./dist/data/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.mts",
      "default": "./dist/plugins/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "default": "./dist/utils/index.mjs"
    },
    "./adapters/*": "./dist/adapters/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format": "bunx prettier --config .prettierrc.cjs '**/*.{js,jsx,ts,tsx}' --write",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx ./src/index.ts",
    "example": "tsx ./src/examples/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
    "@tavily/core": "^0.3.1",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v2-sdk": "^4.8.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.25.2",
    "ajv": "^8.12.0",
    "axios": "^1.8.1",
    "esbuild": "^0.25.0",
    "esbuild-plugin-tsc": "^0.5.0",
    "ethers": "^6.13.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "tslog": "^4.9.3",
    "viem": "^2.22.23",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.6.0",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^9.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.9",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.4.3",
    "simple-git-hooks": "^2.11.1",
    "tinyexec": "^0.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.7",
    "vitest-package-exports": "^0.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
